<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.trackSalesOrder.controller.VendorDetails" xmlns:m="sap.m"
	xmlns:layout="sap.ui.layout" xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<m:Page showHeader="true" title="Vendor Details" showFooter="true" showNavButton="true" navButtonPress="_onPageNavButtonPress">
		<m:content>
			<layout:VerticalLayout width="100%" visible="true">
				<layout:content>
					<m:ObjectListItem intro="" title="{Name}" icon="sap-icon://supplier" number="" numberUnit="" numberState="None" type="Active"
						selected="false" showMarkers="false">
						<m:attributes>
							<m:ObjectAttribute title="Manager" text="{ContactName}" active="false"/>
						</m:attributes>
						<m:firstStatus/>
						<m:secondStatus/>
					</m:ObjectListItem>
				</layout:content>
			</layout:VerticalLayout>
			<form:Form width="100%" editable="true" visible="true">
				<form:title>
					<core:Title text="Address" level="Auto"/>
				</form:title>
				<form:formContainers>
					<form:FormContainer visible="true">
						<form:formElements>
							<form:FormElement visible="true">
								<form:label>
									<m:Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<m:ObjectAttribute text="{Address}" active="false"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<m:Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<m:ObjectAttribute text="{Town}" active="false"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<m:Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<m:ObjectAttribute text="{PostCode}" active="false"/>
								</form:fields>
							</form:FormElement>
						</form:formElements>
						<form:title/>
					</form:FormContainer>
				</form:formContainers>
				<form:layout>
					<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
				</form:layout>
			</form:Form>
			<form:Form width="100%" editable="true" visible="true">
				<form:title>
					<core:Title text="Contact" level="Auto"/>
				</form:title>
				<form:formContainers>
					<form:FormContainer visible="true">
						<form:formElements>
							<form:FormElement visible="true">
								<form:label>
									<m:Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<m:ObjectAttribute text="{PhoneNumber}" active="false"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<m:Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<m:ObjectAttribute text="{Email}" active="false"/>
								</form:fields>
							</form:FormElement>
						</form:formElements>
						<form:title/>
					</form:FormContainer>
				</form:formContainers>
				<form:layout>
					<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
				</form:layout>
			</form:Form>
		</m:content>
		<m:footer>
			<m:Bar design="Auto">
				<m:contentLeft/>
				<m:contentMiddle/>
				<m:contentRight/>
			</m:Bar>
		</m:footer>
		<m:headerContent/>
		<m:subHeader/>
		<m:customHeader/>
	</m:Page>
</mvc:View>